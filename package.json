{
  "name": "unplugin-vue-image",
  "version": "0.1.6-beta.1",
  "description": "Register global imports on demand for Vite and Webpack",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "image"
  ],
  "homepage": "https://github.com/chris-zhu/unplugin-vue-image#readme",
  "bugs": {
    "url": "https://github.com/chris-zhu/unplugin-vue-image/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chris-zhu/unplugin-vue-image.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*",
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./resolvers": {
      "require": "./dist/resolvers.js",
      "import": "./dist/resolvers.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "build": "tsup && nr build:fix",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "test:update": "vitest -u"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "@rollup/pluginutils": "^5.0.2",
    "chokidar": "^3.5.3",
    "local-pkg": "^0.4.3",
    "unplugin": "^1.0.1"
  },
  "devDependencies": {
    "@chris-zhu/eslint-config": "^1.0.7",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "bumpp": "^8.2.1",
    "debug": "^4.3.4",
    "eslint": "^8.33.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "rollup": "^3.12.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-inspect": "^0.7.15",
    "vitest": "^0.28.4"
  }
}
